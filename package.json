{
  "name": "react-homepage",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "node-sass-chokidar": "0.0.1",
    "react-scripts": "0.9.5"
  },
  "dependencies": {
    "prop-types": "^15.5.9",
    "react": "^15.4.2",
    "react-bootstrap": "^0.31.0",
    "react-burger-menu": "^2.0.2",
    "react-dom": "^15.4.2",
    "react-media": "^1.4.0"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy-surge": "npm run build && surge --domain ztoben.surge.sh",
    "deploy-s3": "npm run build && aws s3 sync build/ s3://ztoben",
    "build-css": "node-sass-chokidar src/style/ -o src/css/",
    "watch-css": "npm run build-css && node-sass-chokidar src/style/ -o src/css/ --watch --recursive"
  },
  "homepage": "https://ztoben.github.io/"
}
